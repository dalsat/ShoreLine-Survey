{	#title : 'Questions',	#description : '<p>As a <b>user</b> who found a bug and wants to submit a bug report, how much do you think it is <i>hard</i> to provide the following elements in the description of the defect?</p>',	#questions : [		{			#tag : #description,			#type : #selection,			#question : 'A description of the error'		},		{			#tag : #stacktraces,			#type : #selection,			#question : 'A stack trace generated by the failure'		},		{			#tag : #steps,			#type : #selection,			#question : 'The steps to reproduce the failure'		},		{			#tag : #screenshot,			#type : #selection,			#question : 'A screenshot of the software showing the failure'		},		{			#tag : #code,			#type : #selection,			#question : 'Examples of code to fix the defect'		},		{			#tag : #testcase,			#type : #selection,			#question : 'One or more test cases that generate the defect'		},		{			#tag : #machinedetails,			#type : #selection,			#question : 'The details of the machine (<i>i.e.:</i> hardware) where the failure occurred'		},		{			#tag : #osdetails,			#type : #selection,			#question : 'The details of the operating system where the failure occurred'		},		{			#tag : #envdetails,			#type : #selection,			#question : 'The details of the environment (<i>i.e.:</i> libraries, etc.) where the failure occurred'		},		{			#tag : #assignee,			#type : #selection,			#question : 'The developer that should take care of the problem'		},		{			#tag : #component,			#type : #selection,			#question : 'The component that caused the failure'		},		{			#tag : #version,			#type : #selection,			#question : 'The version of the component that caused the failure'		},		{			#tag : #severity,			#type : #selection,			#question : 'The severity of the defect'		},		{			#tag : #entity,			#type : #selection,			#question : 'The main entity (<i>e.g.:</i> the class) that might contain the defect'		}	]}